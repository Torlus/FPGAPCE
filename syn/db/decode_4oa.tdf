--synthesis_resources = lut 4 
SUBDESIGN decode_4oa
( 
	data[1..0]	:	input;
	enable	:	input;
	eq[3..0]	:	output;
) 
VARIABLE 
	data_wire[1..0]	: WIRE;
	enable_wire	: WIRE;
	eq_node[3..0]	: WIRE;
	eq_wire[3..0]	: WIRE;
	w_anode311w[2..0]	: WIRE;
	w_anode324w[2..0]	: WIRE;
	w_anode332w[2..0]	: WIRE;
	w_anode340w[2..0]	: WIRE;

BEGIN 
	data_wire[] = data[];
	enable_wire = enable;
	eq[] = eq_node[];
	eq_node[3..0] = eq_wire[3..0];
	eq_wire[] = ( w_anode340w[2..2], w_anode332w[2..2], w_anode324w[2..2], w_anode311w[2..2]);
	w_anode311w[] = ( (w_anode311w[1..1] & (! data_wire[1..1])), (w_anode311w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode324w[] = ( (w_anode324w[1..1] & (! data_wire[1..1])), (w_anode324w[0..0] & data_wire[0..0]), enable_wire);
	w_anode332w[] = ( (w_anode332w[1..1] & data_wire[1..1]), (w_anode332w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode340w[] = ( (w_anode340w[1..1] & data_wire[1..1]), (w_anode340w[0..0] & data_wire[0..0]), enable_wire);
END;
